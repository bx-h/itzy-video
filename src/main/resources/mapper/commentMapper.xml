<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hbx.itzy.mapper.CommentMapper">
    <select id="getComment" resultType="String">
        select COMMENT_TEXT from comment where USER_ID = #{userId} and VIDEO_ID = #{videoId}
    </select>


</mapper>